generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model exercices {
  id             Int     @id(map: "exercices_pk") @default(autoincrement())
  nameExerc      String  @unique @db.VarChar(255)
  bodyPart       String  @db.VarChar(255)
  classification String? @db.VarChar(255)
  list           list[]
}

model list {
  idExerc     Int
  idSeries    Int
  idTechnique Int
  id          Int       @id @default(autoincrement())
  exercices   exercices @relation(fields: [idExerc], references: [id])
  series      series    @relation(fields: [idSeries], references: [id])
  technique   technique @relation(fields: [idTechnique], references: [id])
}

model series {
  id           Int    @id @default(autoincrement())
  numberSeries Int
  numberRep    String @db.VarChar(255)
  list         list[]
}

model technique {
  id            Int     @id @default(autoincrement())
  nameTechnique String? @unique @db.VarChar(255)
  description   String? @db.VarChar(4000)
  list          list[]
}
